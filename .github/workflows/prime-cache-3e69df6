name: prime-cache-3e69df6

on:
  workflow_dispatch:

jobs:
  prime_cache:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout code
        uses: actions/checkout@v4

      - name: Cache dependencies (prime-cache-3e69df6)
        id: cache-step
        uses: actions/cache@v4
        with:
          path: ~/.cache
          key: cache-3e69df6

      - name: Echo cache hit/miss (prime-cache-3e69df6)
        run: |
          echo "Cache hit: ${{ steps.cache-step.outputs.cache-hit }}"
